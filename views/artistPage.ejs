<%- include nav.ejs %>
    <div class="container">
        <div class='row'> 
            <div class="col-md-12  mt-3 mb-3">
                <a href="http://127.0.0.1:3000/guitar-chords">Guitar Chords</a><span> / </span><span><%= artist %></span>
            </div>  
            <div class='col-md-8 offset-md-2 mt-3 mb-5'>
                <h6 class="text-center">Can't find the <span class="font-italic"><%= artist %></span> song you're looking for? What about creating it yourself?</h6>
                <a href="add-song/<%= artist %>" id="make-song" type="button" class="btn btn-success d-block mx-auto"><h5>Make Song!</h5></a>
            </div>
            <div id="available-songs" class='col-md-12'>
                <h4 class='text-center mt-2'><%= artist %> Songs</h4>
                <ul id="page-results" class="list-group">
                    <div class="spinner"></div> 
                </ul>
                <nav id="pag-nav" class="mt-3" aria-label="Page navigation example">
                    <ul class="pagination justify-content-center"></ul>
                </nav>
            </div>
        </div>
    </div>
<%- include footer.ejs %>