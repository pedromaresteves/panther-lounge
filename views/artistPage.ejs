<%- include nav.ejs %>
    <div class="container">
        <h1 class='text-center mb-5'><%= artist %></h1>
        <div class='row'>    
            <div class='col-md-8'>
                <h4 class='text-center mt-2'>Available Songs</h4>
                <ul class="list-group">
                    <% data.forEach(function(item){ %>
                        <li class="list-group-item artist-item"><a href="<%= artist %>/<%= item.link %>"><%= item.title %></a><span><a href="edit-song/<%= artistParam %>/<%= item.link %>" class="mr-3">Edit</a><a href="delete-song/<%= artistParam %>/<%= item.link %>">Delete</a></span></li>
                    <%  }); %>  
                </ul>
            </div>
            <div class='col-md-4'>
                <h4 class='text-center mt-2'>The Panther Recomends</h4>
                <ul class="list-group">
                        <% for(var item in recommended){ %>
                            <li class="list-group-item artist-item"><%= recommended[item] %></li>
                        <% } %>
                    </ul>
            </div>
        </div>
    </div>
<%- include footer.ejs %>