<%- include nav.ejs %>

<div class='container'>
    <div class='row justify-content-md-center'>
        <div class='col-md-12 text-center mb-3'>
            <div><img class='rounded-circle img-max img-thumbnail' src='<%= userData.thumbnail %>' alt='Profile Picture' /></div>
            <p class='h3 mt-2'><%= userData.username %></p>
            <p class='mt-0'>
                <strong>Total Songs: </strong><%= userData.username %><br>
                <strong>Today's sexyness level: </strong><%= userData.username %>
            </p>
            <h5 class='mt-5'><strong>Your songs:</strong></h5>
            <ul id='page-results' class='list-group'>
                <div class="spinner"></div> 
            </ul>
            <nav id="pag-nav" class="mt-3" aria-label="Page navigation example">
                <ul class="pagination justify-content-center">
                    <% for(let i = 1; i <= 2; i++) {%>
                        <li class="page-item"><a class="page-link" href="#"><%= i %></a></li>
                    <% }%>
                </ul>
            </nav>
        </div>
    </div>
</div>