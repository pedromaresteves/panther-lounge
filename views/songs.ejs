<%- include nav.ejs %>
<div class="container">
    <div class="row">
        <div class="col-md-12  mt-3 mb-3">
            <a href="http://127.0.0.1:3000/guitar-chords">Guitar Chords</a><span> / </span><a href="http://127.0.0.1:3000/guitar-chords/<%= songData.nArtist %>"><%= songData.artist %></a><span> / </span><span><%= songData.title %><span>
            </div>
            <div class="col-md-8">
                <h2><%= songData.title %> by <a href="http://127.0.0.1:3000/guitar-chords/<%= songData.nArtist %>"><%= songData.artist %></a></h2>
                <p><small>Created By: <%= songData.songCreator %></small></p>
            <div id="editor" class="mt-5" value="<%= songData.lyricsChords %>"></div>
        </div>      
    </div>
</div>
<script src="//cdn.quilljs.com/1.3.6/quill.min.js"></script>
<%- include footer.ejs %>